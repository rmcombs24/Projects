<Window x:Class="MassMediaEditor.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MassMediaEditor"
        mc:Ignorable="d"
        Title="About" Height="200" Width="250"
        ResizeMode="NoResize">

    <Grid>
        <Label x:Name="lblAuthor" Content="Written By: Robert C" HorizontalAlignment="Left" Margin="0,27,0,0" VerticalAlignment="Top" Width="492"/>
        <Label x:Name="lblGitHub" Content="https://github.com/rmcombs24/" HorizontalAlignment="Left" Margin="0,78,0,0" VerticalAlignment="Top" Width="484"/>
        <Label x:Name="lblLinkedin" Content="https://www.linkedin.com/in/rmcombs24/" HorizontalAlignment="Left" Margin="0,51,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.646,3.285" Width="484"/>
        <Label x:Name="lblInfo" Content="MassMediaEditor Version 1.0.0.0" HorizontalAlignment="Left" Margin="0,5,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.646,3.285" Width="484"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" Margin="159,141,0,0" VerticalAlignment="Top" Width="75" Click="BtnClose_Click"/>
    </Grid>
</Window>
